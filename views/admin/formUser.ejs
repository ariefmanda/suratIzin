		<%
			var getRole 	  = ''
			var statusText  = ''
			var statusLabel = ''
			var kelamin     = ''
			if (user) {
				if (user.role == 0) {
					getRole = 'Superadmin'
				} else if (user.role == 1) {
					getRole = 'Admin'
				} else if (user.role == 2) {
					getRole = 'Mitra Jasa'
				} else if (user.role == 3) {
					getRole = 'Mitra Jasa VIP'
				} else if (user.role == 4) {
					getRole = 'Pencari Jasa'
				} else if (user.role == 5) {
					getRole = 'Pencari Jasa VIP'
				}

				if (user.gender == 1) {
					kelamin = 'Pria'
				} else if (user.gender == 2) {
					kelamin = 'Wanita'
				}

				if (user.status == 0) {
					statusText 	= 'Menunggu Aktivasi'
					statusLabel	= 'warning'
				} else if (user.status == 1) {
					statusText 	= 'Aktif'
					statusLabel	= 'success'
				}
			}
		%>
					<div class="row">
		        <div class="col-md-3">

		          <!-- Profile Image -->
		          <div class="box box-primary">
		            <div class="box-body box-profile">
		              <img src="/public/uploads/profile/<%= user.photo %>" onError="this.src='/public/uploads/profile/user.png';" height="392px" width="392px" class="profile-user-img img-responsive img-circle" alt="<%= user.name %>" align="center">
		              <h3 class="profile-username text-center"><%= user.name %></h3>
		              <p class="text-muted text-center"><%= getRole %></p>
		              <!-- <ul class="list-group list-group-unbordered">
		                <li class="list-group-item">
		                  <b>Followers</b> <a class="pull-right">1,322</a>
		                </li>
		                <li class="list-group-item">
		                  <b>Following</b> <a class="pull-right">543</a>
		                </li>
		                <li class="list-group-item">
		                  <b>Friends</b> <a class="pull-right">13,287</a>
		                </li>
		              </ul> -->
		            </div>
		          </div>

		          <!-- About Me -->
		          <div class="box box-primary">
		            <div class="box-header with-border">
		              <h3 class="box-title">Profile Saya</h3>
		            </div>
		            <div class="box-body">

									<table class="table no-border">
				            <tbody>
											<tr>
					              <th style="width:20%">Nama</th>
					              <td><%= user.name %></td>
					            </tr>
					            <tr>
					              <th>Email</th>
					              <td><%= user.email %></td>
					            </tr>
					            <tr>
					              <th>Kelamin</th>
					              <td><%= kelamin %></td>
					            </tr>
					            <tr>
					              <th>No. HP</th>
					              <td><% if (user.handphone != '') { %>+62<% } %><%= user.handphone %></td>
					            </tr>
											<tr>
					              <th>Alamat</th>
					              <td><%= user.address %></td>
					            </tr>
											<tr>
					              <th>Status</th>
					              <td><span class="label label-<%= statusLabel %>"><%= statusText %></span></td>
					            </tr>
				          	</tbody>
									</table>
                  
		            </div>
		          </div>
		        </div>

		        <div class="col-md-9">
		          <div class="nav-tabs-custom">
		            <ul class="nav nav-tabs">
                  <%if(user.role==2||user.role==3){%>
		              <li class="active"><a href="#penawaran" data-toggle="tab">List Penawaran</a></li>
                  <li><a href="#perusahan" data-toggle="tab">List Perusahaan</a></li>
                  <%}%>
		            </ul>
                <div class="tab-content">
                  <%if(user.role==2||user.role==3){%>
		              <div class="active tab-pane" id="penawaran">
                    <table id="dataTable" class="table table-bordered table-striped">
                      <thead>
                        <tr>
                          <th>No</th>
                          <th>Nama Pencari Jasa</th>
                          <th>Lokasi</th>
                          <th>Price</th>
                          <th>Status</th>
                          <th>Nama Perusahaan Anda</th>
                          <th>Tanggal</th>
                        </tr>
                      </thead>
                      <tbody>
                        <tr>
                          <td>No</td>
                          <td>Nama Pencari Jasa</td>
                          <td>Kota</td>
                          <td>Price</td>
                          <td>Status</td>
                          <td>Nama Perusahaan Anda</td>
                          <td>Tanggal</td>
                        </tr>
                      </tbody>
                    </table>
		              </div>
                  <div class="tab-pane" id="perusahan">
                    <table id="dataTable-2" class="table table-bordered table-striped">
                      <thead>
                        <tr>
                          <th>No</th>
                          <th>Nama Perusahaan</th>
                          <th>Nama Category</th>
                          <th>Nama Sub Category</th>
                          <th>Province</th>
                          <th>City</th>
                          <th>email</th>
                        </tr>
                      </thead>
                      <tbody>
                        <tr>
                          <td>No</td>
                          <td>Nama Perusahaan</td>
                          <td>Nama Category</td>
                          <td>Nama Sub Category</td>
                          <td>Province</td>
                          <td>City</td>
                          <td>email</td>
                        </tr>
                      </tbody>
                    </table>
                  </div>
                  <%}else if(user.role==4||user.role==5){%>
                    <table id="dataTable-3" class="table table-bordered table-striped">
                      <thead>
                        <tr>
                          <th>No</th>
                          <th>Remark</th>
                          <th>Provinsi</th>
                          <th>Kota</th>
                          <th>Necessites</th>
                          <th>Status</th>
                          <th>Tanggal</th>
                        </tr>
                      </thead>
                      <tbody>
                        <tr>
                          <td>No</td>
                          <td>Remark</td>
                          <td>Provinsi</td>
                          <td>Kota</td>
                          <td>Necessites</td>
                          <td>Status</td>
                          <td>Tanggal</td>
                        </tr>
                      </tbody>
                    </table>
		              </div>
                  <%}%>
		            </div>
		          </div>
		        </div>
		      </div>
